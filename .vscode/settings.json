{
    "files.autoSave": "onFocusChange",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.defaultFormatter": "formulahendry.code-runner",
    "editor.definitionLinkOpensInPeek": true,
    "editor.experimentalInlineEdit.enabled": true,
    "editor.experimentalInlineEdit.showToolbar": "always",
    "editor.gotoLocation.alternativeDeclarationCommand": "editor.action.revealDeclaration",
    "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.revealDefinitionAside",
    "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToReferences",
    "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
    "editor.guides.bracketPairs": true,
    "editor.linkedEditing": true,
    "editor.mouseWheelZoom": false,
    "editor.multiCursorPaste": "full",
    "editor.occurrencesHighlight": "multiFile",
    "editor.pasteAs.showPasteSelector": "never",
    "editor.peekWidgetDefaultFocus": "editor",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.scrollBeyondLastColumn": 10,
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "mouseover",
    "editor.smoothScrolling": true,
    "editor.stablePeek": true,
    "editor.tabFocusMode": true,
    "editor.unfoldOnClickAfterEndOfLine": true,
    "editor.unicodeHighlight.includeComments": true,
    "editor.unusualLineTerminators": "auto",
    "editor.wordWrap": "on",
    "editor.wrappingStrategy": "simple",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.find.autoFindInSelection": "always",
    "editor.fontSize": 18,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "diffEditor.codeLens": true,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.minimap.scale": 2,
    "editor.minimap.sectionHeaderFontSize": 14,
    "editor.minimap.showSlider": "always",
    "editor.quickSuggestions": {
        "comments": "inline",
        "strings": "inline"
    },
    "editor.suggest.localityBonus": true,
    "editor.suggest.preview": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showStatusBar": true,
    "files.autoGuessEncoding": true,
    "files.autoSaveWorkspaceFilesOnly": true,
    "files.defaultLanguage": "python",
    "files.insertFinalNewline": true,
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "files.trimTrailingWhitespace": true,
    "files.watcherInclude": [
        ".vs code"
    ],
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.list.horizontalScrolling": true,
    "workbench.list.smoothScrolling": true,
    "workbench.colorTheme": "Default High Contrast",
    "workbench.preferredDarkColorTheme": "Default High Contrast",
    "workbench.preferredLightColorTheme": "Default High Contrast Light",
    "workbench.sideBar.location": "right",
    "workbench.editor.autoLockGroups": {
        "default": true
    },
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "long",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.limit.excludeDirty": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.preferHistoryBasedLanguageDetection": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.splitSizing": "auto",
    "workbench.editor.tabSizing": "fit",
    "workbench.editor.titleScrollbarSizing": "large",
    "workbench.editor.untitled.labelFormat": "name",
    "workbench.editor.wrapTabs": true,
    "workbench.settings.editor": "ui",
    "workbench.settings.openDefaultKeybindings": true,
    "screencastMode.keyboardOptions": {
        "showCommandGroups": true
    },
    "screencastMode.mouseIndicatorSize": 35,
    "window.autoDetectColorScheme": false,
    "explorer.excludeGitIgnore": true,
    "explorer.fileNesting.enabled": true,
    "search.defaultViewMode": "tree"
}
